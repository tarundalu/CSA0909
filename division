import java.util.Scanner;
public class Div {
    public static void main(String[] args)
    {
        int a,b,c;
        Scanner scan= new Scanner(System.in);
        System.out.println("enter number: ");
        a=scan.nextInt();
        System.out.println("enter another number: ");
        b=scan.nextInt();
        c=a/b;
        System.out.println("quotient is "+c);
    }
}
