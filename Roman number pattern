public class NumberPattern {
    public static void main(String[] args) {
        int rows = 3; 

        for (int i = 1; i <= rows; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print("I");
            }
            System.out.println();
        }


        for (int i = rows - 1; i >= 1; i--) {
            for (int j = 1; j <= i; j++) {
                System.out.print("I");
            }
            System.out.println();
        }
    }
}
