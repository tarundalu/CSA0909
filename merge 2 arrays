import java.util.Arrays;
public class MergeArrays {
    public static void main(String[] args) {
        int[] array1 = {1, 2, 3};
        int[] array2 = {4, 5, 6};
        System.out.print("merged array is ");
        for(int i=0;i<3;i++)
        {
            System.out.print(array1[i]+",");
            System.out.print(array2[i]+",");
        }
    }
}
