import java.util.Scanner;
public class Mul {
    public static void main(String[] args)
    {
        float a,b,c;
        Scanner obj= new Scanner(System.in);
        System.out.println("enter number: ");
        a=obj.nextFloat();
        System.out.println("enter another number: ");
        b=obj.nextFloat();
        c=a*b;
        System.out.println("product is "+c);
    }
}
